{
    "name": "phpmob/twig-modify-bundle",
    "description": "Spam protect with google reCAPTCHA",
    "keywords": [
        "google",
        "recaptcha",
        "spam-protect"
    ],
    "homepage": "http://github.com/phpmob/recaptcha-bundle",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ishmael Doss",
            "email": "nukboon@gmail.com"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "symfony/form": "^3.4",
        "symfony/validator": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.*"
    },
    "autoload": {
        "psr-4": {
            "PhpMob\\ReCaptchaBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpMob\\ReCaptchaBundle\\spec\\": "spec/"
        }
    }
}
